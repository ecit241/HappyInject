            android:name="me.xiaopan.android.inject.sample.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="me.xiaopan.android.inject.sample.InjectTestActivity"/>
        <activity android:name="me.xiaopan.android.inject.sample.NormalActivity"/>
        <activity android:name="me.xiaopan.android.inject.sample.InjectAdapterActivity"/>
        <activity android:name="me.xiaopan.android.inject.sample.InjectExpandableListAdapterActivity"/>
    </application>

</manifest>
